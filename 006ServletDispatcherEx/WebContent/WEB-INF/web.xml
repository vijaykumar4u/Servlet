<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>006ServletDispatcherEx</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>ReqDispacher</display-name>
    <servlet-name>ReqDispacher</servlet-name>
    <servlet-class>com.vidvaan.dispatcher.ReqDispacher</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ReqDispacher</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>WelcomeLogin</display-name>
    <servlet-name>WelcomeLogin</servlet-name>
    <servlet-class>com.vidvaan.dispatcher.WelcomeLogin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>WelcomeLogin</servlet-name>
    <url-pattern>/welcome</url-pattern>
  </servlet-mapping>
</web-app>